{
  "name": "spotify-karaoke",
  "version": "0.3.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "serve:pl": "ng serve --configuration=pl",
    "build": "ng build",
    "pre:build:prod": "node tools/prepare_secrets.js",
    "build:prod": "npm run pre:build:prod && ng build spotify --aot --buildOptimizer --extractLicenses --no-namedChunks --optimization --outputHashing=all --no-sourceMap --no-vendorChunk",
    "build-storybook": "ng run web-spotify-shared-view:build-storybook",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prepare": "husky install",
    "release": "release-it",
    "extract-i18n": "ng extract-i18n --output-path apps/spotify/src/locale",
    "check-i18n": "ng-i18n-checker ./apps/**/*.html ./libs/**/*.html -i mat-icon",
    "chromatic": "npx chromatic"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.1.1",
    "@angular/cdk": "^11.2.10",
    "@angular/common": "12.1.1",
    "@angular/compiler": "12.1.1",
    "@angular/core": "12.1.1",
    "@angular/forms": "12.1.1",
    "@angular/material": "^11.2.10",
    "@angular/platform-browser": "12.1.1",
    "@angular/platform-browser-dynamic": "12.1.1",
    "@angular/router": "12.1.1",
    "@nrwl/angular": "12.5.8",
    "autoprefixer": "^10.3.1",
    "ngx-cookie-service": "^11.0.2",
    "ngx-google-analytics": "^11.2.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.1.1",
    "@angular-eslint/eslint-plugin": "12.0.0",
    "@angular-eslint/eslint-plugin-template": "12.0.0",
    "@angular-eslint/template-parser": "12.0.0",
    "@angular/cli": "12.1.1",
    "@angular/compiler-cli": "12.1.1",
    "@angular/language-service": "12.1.1",
    "@angular/localize": "^12.1.1",
    "@nrwl/cli": "12.5.8",
    "@nrwl/cypress": "12.5.8",
    "@nrwl/eslint-plugin-nx": "12.5.8",
    "@nrwl/jest": "12.5.8",
    "@nrwl/linter": "12.5.8",
    "@nrwl/storybook": "12.5.8",
    "@nrwl/tao": "12.5.8",
    "@nrwl/workspace": "12.5.8",
    "@release-it/conventional-changelog": "^3.0.1",
    "@storybook/addon-a11y": "^6.3.2",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-docs": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/addon-storysource": "^6.3.4",
    "@storybook/angular": "^6.2.7",
    "@storybook/builder-webpack5": "^6.2.7",
    "@storybook/manager-webpack5": "^6.3.0",
    "@types/jest": "26.0.8",
    "@types/node": "^16.3.2",
    "@types/spotify-api": "0.0.8",
    "@types/spotify-web-playback-sdk": "^0.1.8",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "chromatic": "^5.9.2",
    "cypress": "^6.0.1",
    "dotenv": "10.0.0",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unused-imports": "^1.1.1",
    "husky": "^6.0.0",
    "jest": "26.6.3",
