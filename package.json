    "release-it": "^14.6.1",
    "storybook-dark-mode": "^1.0.8",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "typescript": "4.2.4"
  },
  "lint-staged": {
    "*.{js,json,md,ts,scss,ts,html}": [
      "npm run format --uncommitted"
    ]
  }
}
